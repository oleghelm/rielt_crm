{{ form_start(form) }}

    {{ form_row(form.name) }}
    {{ form_row(form.nameru) }}
    {{ form_row(form.note) }}
    {{ form_row(form.level) }}
    {{ form_row(form.location) }}

    <button class="btn btn-primary" name="submitType" value="apply" type="submit" formnovalidate><i class="fa fa-save"></i></button>&nbsp;
    <button class="btn btn-success" name="submitType" value="save" type="submit" formnovalidate><i class="fa fa-save"></i>&nbsp;<i class="fa fa-plus"></i>&nbsp;<i class="fa fa-sign-out"></i></button>&nbsp;
    {% if(app.request.get('location_id') != '') %}
        <a href="{{path('admin_location_show',{'id':app.request.get('location_id')})}}" class="btn btn-warning"><i class="fa fa-sign-out"></i></a>
    {% else %}
        <a href="{{path('admin_location_list')}}" class="btn btn-warning"><i class="fa fa-sign-out"></i></a>
    {% endif %}
{{ form_end(form) }}